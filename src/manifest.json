{
    "name": "KeyBlock",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Block input keys to window or page.",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["script.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "options_ui": {
        "page": "options.html"
    },
    "permissions": [
        "storage",
        "contextMenus"
    ]
}